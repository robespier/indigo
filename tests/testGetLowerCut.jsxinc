/**
 * Setup
 */
Indigo.Tests.testGetLowerCut = function() {};
Indigo.Tests.testGetLowerCut.prototype = new Indigo.Tests.testSuite(app);
Indigo.Tests.testGetLowerCut.prototype.constructor = Indigo.Tests.testGetLowerCut;
Indigo.Tests.testGetLowerCut.prototype.name = "testGetLowerCut";

Indigo.Tests.testGetLowerCut.prototype.setUp = function () {
	this.pocient = new Indigo.AssemblyImposer(this.app);
	var job = {
		"template" : 'testGetLowerCut',
		"separations" : 'noMatter',
		"roll" : 'noMatter',
		"print_list" : ['noMatter'],
	};
	this.pocient.setup(job);
	this.pocient.templateFolder = this.testsFolder + '/' + this.name;
	this.pocient.openTemplate();
	this.pocient.setLabelLayer();
};

/*
 * Test
 */
Indigo.Tests.testGetLowerCut.prototype.run = function() {
	var lowerCut = this.pocient.getLowerCut();
	var resultIns = this.assertInstanceOf(lowerCut,PathItem);
	$.writeln(resultIns + this.name + '.Type');
	var resultStr = this.assertEq(lowerCut.name,'testTargetPass');
	$.writeln(resultStr + this.name + '.Name');

};
