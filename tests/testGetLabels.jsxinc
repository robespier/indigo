/**
 * Setup
 */
Indigo.Tests.testGetLabels = function() {};
Indigo.Tests.testGetLabels.prototype = new Indigo.Tests.testSuite(app);
Indigo.Tests.testGetLabels.prototype.constructor = Indigo.Tests.testGetLabels;
Indigo.Tests.testGetLabels.prototype.name = "testGetLabels";

Indigo.Tests.testGetLabels.prototype.setUp = function () {
	this.pocient = new Indigo.BaseImposer(this.app);
	var job = {
		"template" : 'noMatter',
		"separations" : 'noMatter',
		"roll" : 'noMatter',
		"print_list" : [
			this.testsFolder + '/testCommons/spaklevka_08_klei.eps',
			this.testsFolder + '/testCommons/spaklevka_1_5_klei.eps',
		],
	};
	this.pocient.setup(job);
};

/**
 * Assert this.pocient is Array of length = 2 
 */
Indigo.Tests.testGetLabels.prototype.run = function() {
	var labels = this.pocient.getLabels();
	var resultAr = this.assertInstanceOf(labels, Array);
	$.writeln(resultAr + this.name + '.Type');
	var resultCn = this.assertEq(labels.length, 2);
	$.writeln(resultCn + this.name + '.Length');
};

/**
 * Superclass override: do nothing;
 */
Indigo.Tests.testGetLabels.prototype.tearDown = function() {
};
